@model AgendamentoServicos
@using Microsoft.AspNetCore.Http;


@{
    ViewData["Title"] = "Incluir Serviços";
} 


<h1>@ViewData["Title"]</h1>

<p class="text-danger">@ViewData["mensagem"]</p>

<form method="post" asp-action="incluir">

     <p>
        <label for="Nome">Nome</label>
        <input asp-for = "Nome">
    </p>
      <p>
        <label for="DataSolicitacao ">Data da Solicitacao</label>
        <input asp-for = "DataSolicitacao ">
    </p>
     <p>
        <label for="Servico">Serviço</label>
        <input asp-for = "Servico">
    </p>   
     
     <p>
        <input type="submit" value="Alterar Serviços">   
    </p>
    <input value=@Context.Session.GetInt32("IdUsuario") type="hidden">
 </form>