@model List<Agendamento>

<h2>Lista de Agendamentos</h2>

<table>
  <style>
    table,
    th,
    td {
      border: 2px solid black;
    }
  </style>
  <tr>
    <th>Nome</th>
    <th>Telefone</th> <br>
    <th>Data</th><br>
    <th>Animal</th><br>
    <th>Necessidade</th><br>
  </tr>
  @foreach (Agendamento a in Model)
  {
    <tr>

      <td>@a.nome</td>
      <td>@a.telefone</td>
      <td>@a.data</td>
      <td>@a.animal</td>
      <td>@a.necessidade</td>
    </tr>
  }
</table>
Total de Agendamentos Cadastrados: @Dados.AgendamentoAtual.TotalizarAgendamento()