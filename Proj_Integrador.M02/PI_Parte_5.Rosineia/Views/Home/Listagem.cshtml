@model List<Agendamento>

<h4>Serviço solicitado com Sucesso!!</h4>

<table>
  <style>
    table,
    th,
    td {
      border: 2px solid black;
    }
  </style>

   @foreach (Agendamento a in Model)
  { 

    <tr>
    <th>Nome</th>
    <th>Telefone</th> <br>
    <th>Data</th><br>
    <th>Descrição</th><br>
    <th>Serviço</th><br>
  </tr>
    <tr>

      <td>@a.nome</td>
      <td>@a.telefone</td>
      <td>@a.data.ToString("dd/MM/yyyy")</td>
      <td>@a.Descrição</td>
      <td>@a.Serviço</td>
    </tr>
    
  }
</table><br>
Retornar para página pricipal <a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>